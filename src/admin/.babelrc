{
	"presets": [
		[
			"env",
			{
				"modules": false,
				"targets": {
					"browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
				}
			}
		],
    "@babel/preset-env",
    "@babel/preset-react",
		"react",
		"stage-2"
	],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}